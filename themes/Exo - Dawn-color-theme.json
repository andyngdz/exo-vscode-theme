        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "variable.parameter",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#a85000"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
