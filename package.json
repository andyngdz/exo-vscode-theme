{
  "name": "exo-vscode-theme",
  "displayName": "Exo Theme",
  "description": "A muted, eye-friendly theme family for VS Code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Exo - Dusk",
        "uiTheme": "vs-dark",
        "path": "./themes/Exo - Dusk-color-theme.json"
      },
      {
        "label": "Exo - Dawn",
        "uiTheme": "vs-dark",
        "path": "./themes/Exo - Dawn-color-theme.json"
      },
      {
        "label": "Exo - Midnight",
        "uiTheme": "vs-dark",
        "path": "./themes/Exo - Midnight-color-theme.json"
      },
      {
        "label": "Exo - Eclipse",
        "uiTheme": "vs-dark",
        "path": "./themes/Exo - Eclipse-color-theme.json"
      }
    ]
  },
  "scripts": {
    "build": "tsx src/build.ts",
    "watch": "tsx watch src/build.ts"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "colord": "^2.9.3"
  }
}
