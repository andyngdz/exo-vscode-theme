{
  "name": "exo-vscode-theme",
  "displayName": "Exo Theme",
  "description": "A muted, eye-friendly theme family for VS Code",
  "version": "0.1.1",
  "publisher": "exohq",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/andyngdz/exo-vscode-theme"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "dark theme",
    "color theme",
    "eye-friendly",
    "muted"
  ],
  "icon": "logo.png",
  "galleryBanner": {
    "color": "#1a1520",
    "theme": "dark"
  },
  "contributes": {
    "themes": [
      {
        "label": "Exo - Dusk",
        "uiTheme": "vs-dark",
        "path": "./themes/Exo - Dusk-color-theme.json"
      },
      {
        "label": "Exo - Dawn",
        "uiTheme": "vs-dark",
        "path": "./themes/Exo - Dawn-color-theme.json"
      },
      {
        "label": "Exo - Midnight",
        "uiTheme": "vs-dark",
        "path": "./themes/Exo - Midnight-color-theme.json"
      },
      {
        "label": "Exo - Eclipse",
        "uiTheme": "vs-dark",
        "path": "./themes/Exo - Eclipse-color-theme.json"
      },
      {
        "label": "Exo - Forest",
        "uiTheme": "vs-dark",
        "path": "./themes/Exo - Forest-color-theme.json"
      }
    ]
  },
  "scripts": {
    "build": "tsx src/build.ts",
    "watch": "tsx watch src/build.ts",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "colord": "^2.9.3",
    "tsx": "^4.7.0",
    "typescript": "^5.0.0"
  }
}
